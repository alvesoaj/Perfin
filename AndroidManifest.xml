        <activity android:name=".view.ListCartaoActivity"></activity>
        <activity android:name=".view.ListChequeActivity"></activity>
        <activity android:name=".view.ListDinheiroActivity"></activity>
        <activity android:name=".view.ListRendaActivity"></activity>
